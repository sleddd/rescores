{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "body-copy",
					"color": "#0f172a",
					"name": "Body Copy"
				},
				{
					"slug": "gray-lt",
					"color": "#f1f5f9",
					"name": "Gray - Lt"
				},
				{
					"slug": "gray-md",
					"color": "#cbd5e1",
					"name": "Gray - Md"
				},
				{
					"slug": "gray-dark",
					"color": "#475569",
					"name": "Gray - Dark"
				},
				{
					"slug": "link-active",
					"color": "#1e3a8a",
					"name": "Primary - Link Active"
				},
				{
					"slug": "link-hover",
					"color": "#357dc0",
					"name": "Primary Lt - Link Hover"
				},
				{
					"slug": "link-visited",
					"color": "#2b2788",
					"name": "Link Visited"
				},
				{
					"slug": "link-focus",
					"color": "#254a87",
					"name": "Link Focus"
				},
				{
					"slug": "secondary",
					"color": "#da804f",
					"name": "Secondary"
				},
				{
					"slug": "tertiary-red",
					"color": "#c7344c",
					"name": "Tertiary - Red"
				},
				{
					"slug": "tertiary_green",
					"color": "#25903c",
					"name": "Tertiary - Green"
				},
				{
					"slug": "gold",
					"color": "#dcb84f",
					"name": "Gold"
				}
			]
		},
		"layout": {
			"contentSize": "80%",
			"wideSize": "80%"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true
		}
	}
}

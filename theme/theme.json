{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "body-copy",
					"color": "#0f172a",
					"name": "Body Copy"
				},
				{
					"slug": "gray-lt",
					"color": "#f1f5f9",
					"name": "Gray - Lt"
				},
				{
					"slug": "gray-md",
					"color": "#cbd5e1",
					"name": "Gray - Md"
				},
				{
					"slug": "gray-dark",
					"color": "#475569",
					"name": "Gray - Dark"
				},
				{
					"slug": "gray-darkest",
					"color": "#555555",
					"name": "Gray - Darkest"
				},
				{
					"slug": "link-active",
					"color": "#141f6c",
					"name": "Primary - Link Active"
				},
				{
					"slug": "link-hover",
					"color": "#617fd6",
					"name": "Primary Lt - Link Hover"
				},
				{
					"slug": "link-visited",
					"color": "#2b2788",
					"name": "Link Visited"
				},
				{
					"slug": "link-focus",
					"color": "#254a87",
					"name": "Link Focus"
				},
				{
					"slug": "secondary",
					"color": "#da804f",
					"name": "Secondary"
				},
				{
					"slug": "tertiary-red",
					"color": "#a63514",
					"name": "Tertiary - Red"
				},
				{
					"slug": "tertiary_green",
					"color": "#168A97",
					"name": "Tertiary - Green"
				},
				{
					"slug": "gold",
					"color": "#C9A63F",
					"name": "Gold"
				},
				{
					"slug": "white",
					"color": "#fff",
					"name": "White"
				}
			]
		},
		"custom": {
            "fontWeight": {
                "light": 300,
				"regular": 400,
                "medium": 500,
                "semi-bold": 600,
				"bold": 700
            },
            "lineHeight": {
                "body": 1.5,
                "heading": 1.2,
                "medium": 1.5,
                "one": 1
            },
            "spacing": {
                "outer": "30px"
            }
        },
		"layout": {
			"contentSize": "80%",
			"wideSize": "80%"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "10px",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "15px",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "25px",
					"slug": "30",
					"name": "2"
				},
				{
					"size": "30px",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "35px",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "40px",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "45px",
					"slug": "70",
					"name": "7"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": true,
			"fluid": true, 
			"fontFamilies": [
				{
					"fontFamily": "Cormorant Garamond",
					"name": "Cormorant Garamond",
					"slug": "cormorant-garamond",
					"fontFace": [
						{
							"fontFamily": "Cormorant Garamond",
							"fontWeight": "300",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-Light.ttf"				
							]
						},
						{
							"fontFamily": "Cormorant Garamond",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-Regular.ttf"				
							]
						},
						{
							"fontFamily": "Cormorant Garamond",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-Medium.ttf"				
							]
						}, 
						{
							"fontFamily": "Cormorant Garamond",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-SemiBold.ttf"				
							]
						}, 
						{
							"fontFamily": "Cormorant Garamond",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-Bold.ttf"				
							]
						},
						{
							"fontFamily": "Cormorant Garamond",
							"fontWeight": "300",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-LightItalic.ttf"				
							]
						},
						{
							"fontFamily": "Cormorant Garamond",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-RegularItalic.ttf"				
							]
						},
						{
							"fontFamily": "Cormorant Garamond",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-MediumItalic.ttf"				
							]
						}, 
						{
							"fontFamily": "Cormorant Garamond",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-SemiBoldItalic.ttf"				
							]
						}, 
						{
							"fontFamily": "Cormorant Garamond",
							"fontWeight": "700",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/Cormorant/CormorantGaramond-BoldItalic.ttf"				
							]
						}
					]
				},
				{
					"fontFamily": "Lato",
					"name": "Lato",
					"slug": "lato",
					"fontFace": [
						{
							"fontFamily": "Lato",
							"fontWeight": "300",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Lato/Lato-Light.ttf"				
							]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/Lato/Lato-Regular.ttf"				
							]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Lato/Lato-Bold.ttf"				
							]
						}, 
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/Lato/Lato-Black.ttf"					
							]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "300",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/Lato/Lato-LightItalic.ttf"				
							]
						},
						{
							"fontFamily": "Lato",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/Lato/Lato-Italic.ttf"				
							]
						},
						{
							"fontFamily": "Lato",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Lato/Lato-BoldItalic.ttf"				
							]
						}, 
						{
							"fontFamily": "Lato",
							"fontStyle": "italic",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/Lato/Lato-BlackItalic.ttf"					
							]
						}
					]
				}
			],
			"fontSizes": [
                {
                    "fluid": {
                        "max": "3.5rem",
                        "min": "3rem"
                    },
                    "name": "x-large",
                    "size": "3rem",
                    "slug": "x-large"
                }, 
				{
                    "fluid": {
                        "max": "3rem",
                        "min": "2.5rem"
                    },
                    "name": "large",
                    "size": "2.5rem",
                    "slug": "large"
                }, 
				{
                    "fluid": {
                        "max": "2.5rem",
                        "min": "2rem"
                    },
                    "name": "medium",
                    "size": "2rem",
                    "slug": "medium"
                }, 
				{
                    "fluid": {
                        "max": "2rem",
                        "min": "1.5rem"
                    },
                    "name": "medium-small",
                    "size": "1.5rem",
                    "slug": "medium-small"
                }, 
				{
                    "fluid": {
                        "max": "1rem",
                        "min": "1rem"
                    },
                    "name": "small",
                    "size": "1rem",
                    "slug": "small"
                }, 
				{
					"fluid": {
                        "max": "1.5rem",
                        "min": "1rem"
                    },
                    "name": "x-small",
                    "size": "1rem",
                    "slug": "x-small"
                }, 
				{
                    "name": "tiny",
                    "size": ".95rem",
                    "slug": "tiny"
                }
            ]
		},
		"useRootPaddingAwareAlignments": true
	}, 
	"styles": {
		"spacing": {
			"margin":"0",
            "padding": "0",
            "blockGap":"0"
		},
		"typography": {
            "fontFamily": "var(--wp--preset--font-family--lato)",
            "fontSize": "16px",
            "fontWeight": "var(--wp--custom--font-weight--regular)",
            "lineHeight": "var(--wp--custom--line-height--body)"
        },
		"elements": {
			"h1": {
				"typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": 600, 
					"fontFamily": "Cormorant Garamond"
                }			
			},
			"h2": {
				"typography": {
                    "fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": 600, 
					"fontFamily": "Cormorant Garamond"
                }
			},
			"h3": {
				"typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": 600, 
					"fontFamily": "Cormorant Garamond"
                }
			},
			"h4": {
				"typography": {
                    "fontSize": "var(--wp--preset--font-size--medium-small)",
					"fontWeight": 500, 
					"fontFamily": "Cormorant Garamond"
                }
			},
			"h5": {
				"typography": {
                    "fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": 500, 
					"fontFamily": "Cormorant Garamond"
                }
			},
			"h6": {
				"typography": {
                    "fontSize": "var(--wp--preset--font-size--tiny)",
					"fontWeight": 600, 
					"fontFamily": "Lato",
					"textTransform": "uppercase",
					"letterSpacing": "0.05rem"
                },
				"spacing": {
					"padding": {
						"top": ".45rem",
						"bottom": ".45rem"
					}
				}
			},
			"button": {
				"border": {
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--link-active)",
					"text": "var(--wp--preset--color--white)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--link-hover) !important",
						"text": "var(--wp--preset--color--white)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--link-active)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--link-active)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--white)"
					}
				}
			},
			"figure" : {
				"spacing" : {
					"margin" : {
						"top": "0.5rem",
						"bottom": "0.5rem",
						"left": "0",
						"right": "0"
					},
					"padding" : {
						"top": "0.5rem",
						"bottom": "0.5rem",
						"left": "0",
						"right": "0"
					}
				}
			},
			"link" : {
				"color": {
					"text": "var(--wp--preset--color--link-active)"
				},
				"typography": {
					"textDecoration": "none"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--link-hover)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--link-focus)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--link-active)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			},
			"p": {
				"spacing": {
					"padding" : {
						"top": "0.5rem",
						"bottom": ".45rem",
						"right": "0",
						"left": "0"
					}
				}
			},
			"pre" : {
				"color": {
					"background" : "#f1f5f9"
				},
				"border": {
					"color": "#cbd5e1",
					"width": "1px",
					"style": "solid"
				}
			},
			"code" : {
				"color": {
					"background": "#f1f5f9"
				},
				"typography" : {
					"display": "block",
					"borderRadius": "3px"
				},
				"spacing": {
					"padding" : {
						"top":"1rem",
						"bottom": "1rem",
						"right": "1.5rem",
						"left": "1.5rem"
					},
					"margin" :  {
						"top":"1rem",
						"bottom": "1rem",
						"right": "0rem",
						"left": "0rem"
					}
				},
				"border": {
					"color": "#cbd5e1",
					"width": "1px",
					"style": "solid"
				}
			}
		},
		"blocks": {
			"core/navigation": {
                "typography": {
					"fontSize": ".85rem",
					"letterSpacing": "0.0rem",
                    "textTransform": "uppercase"
                },
				"elements" : {
					"link": {
						"typography" : {
							"textDecoration": "none"
						},
						":hover" : {
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
            },
			"core/site-title": {
                "typography": {
					"fontSize": "1.5rem",
					"letterSpacing": "0.0rem"
                },
				"elements" : {
					"h1": {
						"typography": {
							"fontFamily": "'Lato', san-serif",
							"fontSize": "1.5rem"
						}
					},
					"link": {
						"typography": {
							"fontFamily": "'Lato', san-serif",
							"fontSize": "1.5rem"
						}
					}
				}
            },
			"core/site-tagline": {
                "typography": {
					"fontSize": ".85rem",
					"letterSpacing": "0.0rem",
                    "fontStyle": "italic",
					"lineHeight":"1.3"
                }
            },
			"core/audio": {
                "spacing": {
					"padding" : {
						"top": "1rem",
						"bottom": "1rem",
						"left": "0",
						"right": "0"
					}
                }
            },
			"core/columns": {
                "spacing": {
					"blockGap": "2rem"
                }
            },
			"core/quote": {
				"spacing": {
					"padding": {
						"top": "1.5rem",
						"bottom": "1.5rem",
						"left": "1rem",
						"right": "1rem"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--link-active) !important"
				},
				"spacing": {
					"margin": {
						"top": "3rem",
						"bottom": "3rem",
						"left": "0",
						"right": "0"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
